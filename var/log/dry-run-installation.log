CREATE TABLE `magemastery_todo_task` (
`task_id` int  NOT NULL  AUTO_INCREMENT COMMENT "Task ID", 
`label` varchar(255) NOT NULL  COMMENT "Task Label", 
`status` varchar(255) NOT NULL DEFAULT "open" COMMENT "Task Status", 
`customer_id` int UNSIGNED NOT NULL   COMMENT "Customer ID", 
CONSTRAINT  PRIMARY KEY (`task_id`), 
CONSTRAINT `MAGEMASTERY_TODO_TASK_CUSTOMER_ID_CUSTOMER_ENTITY_ENTITY_ID` FOREIGN KEY (`customer_id`) REFERENCES `customer_entity` (`entity_id`)  ON DELETE CASCADE
) ENGINE=innodb DEFAULT CHARSET=utf8 DEFAULT COLLATE=utf8_general_ci COMMENT="TODO tasks"

